<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="Theme.NoTitleBar.Fullscreen" parent="@android:style/Theme.Light.NoTitleBar.Fullscreen">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@drawable/welcome</item>
    </style>
    <!-- 登陆注册 -->
    <style name="loginb_txt">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:paddingBottom">12dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingLeft">60dp</item>
        <item name="android:paddingRight">60dp</item>
        <item name="android:background">@drawable/loginb_txt_bg</item>
    </style>

    <style name="register_txt">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingLeft">40dp</item>
        <item name="android:paddingRight">40dp</item>
        <item name="android:background">@drawable/loginb_txt_bg</item>
    </style>
    <!-- 登陆注册end -->


    <!-- 装女郎 排行榜 -->
    <style name="woman_rank_name">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#2f2f2f</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="woman_vote_num">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#7e7e7e</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:gravity">right</item>
    </style>
    <!-- 装女郎 排行榜end -->


    <!-- 装女郎  评选 -->
    <style name="vote_item_text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#2f2f2f</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingTop">2dp</item>
    </style>
    <!-- 装女郎 评选end -->


    <!-- 装女郎 评选后 页面 -->
    <style name="vote_after_left_text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="vote_after_right_text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:gravity">right</item>
        <item name="android:background">@drawable/vote_after_text_corner</item>
        <item name="android:alpha" tools:ignore="NewApi">0.5</item>
    </style>
    <!-- 装女郎 评选后 页面 end -->
    <!-- 装女郎 报名详情页 -->
    <style name="sign_info_item">
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="sign_info_text_gray">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#b1b1b1</item>
    </style>

    <style name="sign_info_text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#565656</item>
        <item name="android:layout_marginRight">0dp</item>
    </style>
    <!-- 装女郎 报名详情页end -->


    <!-- 我的页面 -->
    <style name="mine_container_b">
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:paddingRight">3dp</item>
        <item name="android:background">@drawable/mine_container_b_corner</item>
    </style>

    <style name="mine_container_s">
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingLeft">14dp</item>
        <item name="android:paddingRight">18dp</item>
        <item name="android:background">@drawable/mine_container_s_corner</item>
    </style>

    <style name="mine_container_s_noborder">
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingLeft">14dp</item>
        <item name="android:paddingRight">18dp</item>
    </style>
    <!-- 我的页面end -->


    <!-- 菜单文字 -->
    <style name="menu_txt">
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:drawablePadding">15dp</item>
    </style>
	
    <!-- win menu -->
    <style name="win_menu_bt">
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:textColor">#47a3ff</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/win_menu_bt_bg</item>
    </style>
    <style name="mystyle" parent="android:Animation">  
        <item name="@android:windowEnterAnimation">@anim/dialog_enter</item>  
        <item name="@android:windowExitAnimation">@anim/dialog_exit</item>     
    </style>  
    
</resources>