<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    <!-- 装女郎 排行榜 -->
    <style name="woman_rank_name">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#2f2f2f</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:gravity">left</item>
    </style>
    
    <style name="woman_vote_num">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#7e7e7e</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:gravity">right</item>
    </style>
    <!-- 装女郎 排行榜end -->
    
    <!-- 装女郎  评选 -->
	<style name="vote_item_text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#2f2f2f</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingTop">2dp</item>
    </style>
    <!-- 装女郎 评选end -->
    
    <!-- 装女郎 评选后 页面 -->
    <style name="vote_after_left_text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:gravity">left</item>
    </style>
    <style name="vote_after_right_text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#fff</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:gravity">right</item>
        <item name="android:background">@drawable/vote_after_text_corner</item>
        <item name="android:alpha" tools:ignore="NewApi">0.5</item>
    </style>
    <!-- 装女郎 评选后 页面 end-->
</resources>